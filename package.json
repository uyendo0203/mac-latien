{
  "author": "uyendo23",
  "name": "uyendo23",
  "version": "1.0",
  "description": "Starter project template boilerplate based on Webpack with SASS/PostCSS and babel ES6/7 support.",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.4.4",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.0.0",
    "sass": "^1.50.1",
    "sass-lint": "^1.13.1",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean-nm": "rm -rf node_modules/",
    "clean-build": "rmdir /s /q dist && npm run build",
    "clean-dist": "rmdir /s /q dist",
    "clean-rm-dist": "rm -rf dist",
    "bundle": "npm install && npm run watch",
    "build": "webpack --config configuration/webpack.dev.config.js --mode=development",
    "watch": "webpack --config configuration/webpack.dev.config.js --mode=development --watch",
    "start": "webpack serve --config configuration/webpack.dev.config.js --mode=development",
    "prod": "webpack --config configuration/webpack.prod.config.js --mode=development",
    "lint:sass": "sass-lint -v -q",
    "lint:js": "eslint --ext .js src/js/",
    "stats": "webpack --config configuration/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "run-server": "cd dist && npx http-server"
  },
  "target": "web",
  "dependencies": {
    "http-server": "^14.1.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^16.0.2",
    "sass-migrator": "^2.2.0",
    "twig-html-loader": "^0.1.9",
    "twig-loader": "^0.5.5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
